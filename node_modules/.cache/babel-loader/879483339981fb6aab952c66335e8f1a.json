se\"],\n  setup(props, { emit }) {\n    function drawShape(shape) {\n      console.log(\"draw shape : \", shape);\n      emit(\"drawShape\", shape);\n    }\n    function reverse() {\n      emit(\"reverse\");\n    }\n    onMounted(() => {\n      console.log(\"SHAPE : \");\n    });\n\n    return { SHAPES, drawShape, reverse };\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}