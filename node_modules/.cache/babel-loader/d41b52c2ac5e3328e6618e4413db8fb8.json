eetProjectDetail\",\n  props: [\"projectName\"],\n  setup(props) {\n    const store = useStore();\n    const project = computed(() =>\n      store.getters[\"project/getProjectByName\"](props.projectName)\n    );\n    console.log(\"PROJECT NAME\", project);\n\n    return {\n      project,\n    };\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}