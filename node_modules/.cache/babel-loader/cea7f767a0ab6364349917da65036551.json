{"ast":null,"code":"import * as PIXI from \"pixi.js\";\nimport Orb from \"../helpers/Orb\";\nimport { KawaseBlurFilter } from \"@pixi/filter-kawase-blur\";\nexport default {\n  name: \"SweetHomepage\",\n  components: {},\n  methods: {\n    goToProjects() {\n      this.$router.push('/projects');\n    }\n\n  },\n\n  mounted() {\n    const app = new PIXI.Application({\n      view: document.querySelector(\".orb-canvas\"),\n      resizeTo: window,\n      backgroundAlpha: 0\n    });\n    app.stage.filters = [new KawaseBlurFilter(30, 20, true)];\n    const orbs = [];\n    let originX = window.innerWidth / 1.25;\n    let originY = window.innerWidth < 1000 ? window.innerHeight : window.innerHeight / 1.375;\n\n    for (let i = 0; i < 10; i++) {\n      const orb = new Orb(0xff0000, originX, originY);\n      app.stage.addChild(orb.graphics);\n      orbs.push(orb);\n    }\n\n    originX = window.innerWidth * 0.2;\n    originY = window.innerWidth < 1000 ? 0 : window.innerHeight * 0.275;\n\n    for (let i = 0; i < 10; i++) {\n      const orb = new Orb(0x0000ff, originX, originY);\n      app.stage.addChild(orb.graphics);\n      orbs.push(orb);\n    }\n\n    if (!window.matchMedia(\"(prefers-reduced-motion: reduce)\").matches) {\n      app.ticker.add(() => {\n        orbs.forEach(orb => {\n          orb.update();\n          orb.render();\n        });\n      });\n    } else {\n      orbs.forEach(orb => {\n        orb.update();\n        orb.render();\n      });\n    }\n  }\n\n};","map":{"version":3,"mappings":"AAqCA,OAAO,KAAKA,IAAZ,MAAsB,SAAtB;AACA,OAAOC,GAAP,MAAgB,gBAAhB;AACA,SAASC,gBAAT,QAAiC,0BAAjC;AAEA,eAAe;AACbC,MAAI,EAAE,eADO;AAEbC,YAAU,EAAE,EAFC;AAGbC,SAAO,EAAC;AACPC,gBAAY,GAAE;AACd,WAAKC,OAAL,CAAaC,IAAb,CAAkB,WAAlB;AACG;;AAHI,GAHK;;AAQbC,SAAO,GAAG;AACR,UAAMC,GAAE,GAAI,IAAIV,IAAI,CAACW,WAAT,CAAqB;AAC/BC,UAAI,EAAEC,QAAQ,CAACC,aAAT,CAAuB,aAAvB,CADyB;AAE/BC,cAAQ,EAAEC,MAFqB;AAG/BC,qBAAe,EAAE;AAHc,KAArB,CAAZ;AAMAP,OAAG,CAACQ,KAAJ,CAAUC,OAAV,GAAoB,CAAC,IAAIjB,gBAAJ,CAAqB,EAArB,EAAyB