ild(orb.graphics);\n\n      orbs.push(orb);\n    }\n\n    if (!window.matchMedia(\"(prefers-reduced-motion: reduce)\").matches) {\n      app.ticker.add(() => {\n        orbs.forEach((orb) => {\n          orb.update();\n          orb.render();\n        });\n      });\n    } else {\n      orbs.forEach((orb) => {\n        orb.update();\n        orb.render();\n      });\n    }\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}