{"ast":null,"code":"import p5 from \"p5\";\nimport Orb from \"../helpers/Orb\";\nimport { onMounted } from \"@vue/runtime-core\";\nexport default {\n  name: \"SweetBackground\",\n\n  setup() {\n    onMounted(() => {\n      const sketch = s => {\n        let w = window.innerWidth;\n        let h = window.innerHeight;\n        let orbs = [];\n\n        s.setup = () => {\n          s.createCanvas(w, h);\n          s.noStroke();\n          orbs = initOrbs();\n        };\n\n        s.draw = () => {\n          s.background(0);\n\n          for (const orb of orbs) {\n            orb.move(s);\n            orb.render(s);\n          }\n        };\n\n        function initOrbs() {\n          const orbsArray = [];\n          let xPosInWidthPct = 0.7;\n          let yPosInHeightPct = 0.7;\n          let maxDistPct = 0.3;\n\n          for (let i = 0; i < 10; i++) {\n            const orb = new Orb(xPosInWidthPct, yPosInHeightPct, maxDistPct, s.color(\"#ff0000\"));\n            orbsArray.push(orb);\n          }\n\n          xPosInWidthPct = 0.3;\n          yPosInHeightPct = 0.3;\n\n          for (let i = 0; i < 10; i++) {\n            const orb = new Orb(xPosInWidthPct, originY, maxDistPct, s.color(\"#0000ff\"));\n            orbsArray.push(orb);\n          }\n\n          return orbsArray;\n        }\n\n        window.onresize = function () {\n          // assigns new values for width and height variables\n          w = window.innerWidth;\n          h = window.innerHeight;\n          s.resizeCanvas(w, h);\n          initOrbs();\n        };\n      };\n\n      const sketchInstance = new p5(sketch, \"orb-sketch\");\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAQA,OAAOA,EAAP,MAAe,IAAf;AACA,OAAOC,GAAP,MAAgB,gBAAhB;AACA,SAASC,SAAT,QAA0B,mBAA1B;AAEA,eAAe;AACbC,MAAI,EAAE,iBADO;;AAEbC,OAAK,GAAG;AACNF,aAAS,CAAC,MAAM;AACd,YAAMG,MAAK,GAAKC,CAAD,IAAO;AACpB,YAAIC,IAAIC,MAAM,CAACC,UAAf;AACA,YAAIC,IAAIF,MAAM,CAACG,WAAf;AAEA,YAAIC,IAAG,GAAI,EAAX;;AAEAN,SAAC,CAACF,KAAF,GAAU,MAAM;AACdE,WAAC,C