icle>\n    </main>\n  </div>\n</template>\n\n<script>\nimport { ref } from \"@vue/reactivity\";\nimport { mapState } from \"vuex\";\nimport { onMounted } from \"@vue/runtime-core\";\nimport { getLastUrlItem } from \"@/helpers/utils\";\nimport { useRoute } from \"vue-router\";\n\nexport default {\n  name: \"SweetProjectDetail\",\n  props: [\"projectName\"],\n  setup(props) {\n    const route = useRoute();\n\n    const project = computed(() =>\n      store.getters[\"project/getProjectByName\"](props.projectName)\n    );\n    return {\n      project,\n    };\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}